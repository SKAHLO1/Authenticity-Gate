# Firebase Configuration (REQUIRED - for data storage and authentication)
# Get from Firebase Console > Project Settings > Service Accounts
FIREBASE_PROJECT_ID=
FIREBASE_SERVICE_ACCOUNT=

# Firebase Client Configuration (REQUIRED - for client-side authentication)
# Get from Firebase Console > Project Settings > General > Your apps > Web app
# These are safe to expose in client-side code
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=

# GenLayer Intelligent Contract (REQUIRED - this is how AI analysis is performed)
# Deploy the contract first (see contracts/README.md)
# The contract uses Gemini AI via GenLayer's consensus mechanism
GENLAYER_CONTRACT_ADDRESS=
GENLAYER_RPC_URL=https://studio.genlayer.com:8000
GENLAYER_PRIVATE_KEY=

# AI Fallback (optional - used if GenLayer contract is not configured)
# Get from Google AI Studio: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=

# Queue System (optional - uses in-memory queue if not provided)
# For production, use Redis for persistent queues
REDIS_URL=

# Security (optional - defaults provided for development)
SESSION_SECRET=
CORS_ORIGIN=

# Rate Limiting (optional - uses defaults if not set)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging (optional - defaults to 'info')
LOG_LEVEL=info
